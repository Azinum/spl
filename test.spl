// test.spl

fn foo 12;
fn bar 17;
fn baz + 2 3;

fn main {
  memory tmp 8;
  memory funcs 16;
  = (+0 @funcs) @foo;
  = (+8 @funcs) @bar;
  = (+16 @funcs) @baz;
  const count 3;
  const i @tmp;
  const ptr @funcs;
  while < #i count {
    const func #ptr;
    print func();
    = ptr + 8 #ptr;
    = i + 1 #i;
  }
  0;
}
