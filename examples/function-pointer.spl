// function-pointer.spl

fn foo(x : u64) -> u64 + x 7;
fn bar(x : u64) -> u64 * x x;
fn baz(x : u64) -> u64 + x x;

fn main -> none {
  const count 3;
  let funcs (foo, bar, baz);
  let i 0;
  let index 0;
  let func foo;
  while < i count {
    = @func load64 + index @funcs;
    print func(i);
    = @i + 1 i;
    = @index + sizeof u64 index;
  }
}
